<body>
<!-- container section start -->
<section id="container" class="">


    <header class="header dark-bg">
        <div class="toggle-nav">
            <div class="icon-reorder tooltips" data-original-title="Toggle Navigation" data-placement="bottom"><i class="icon_menu"></i></div>
        </div>

        <!--logo start-->
        <a href="index.html" class="logo">Reput <span class="lite">GIT</span></a>
        <!--logo end-->

        <div class="nav search-row" id="top_menu">
            <!--  search form start -->
            <!--  search form end -->
        </div>

        <div class="top-nav notification-row">
            <!-- notificatoin dropdown start-->
            <ul class="nav pull-right top-menu">

                <!-- task notificatoin start -->
                <!-- task notificatoin end -->
                <!-- inbox notificatoin start-->

                <!-- inbox notificatoin end -->
                <!-- alert notification start-->

                <!-- alert notification end-->
                <!-- user login dropdown start-->

                <!-- user login dropdown end -->
            </ul>
            <!-- notificatoin dropdown end-->
        </div>
    </header>
    <!--header end-->

    <!--sidebar start-->
    <aside>
        <div id="sidebar"  class="nav-collapse ">
            <!-- sidebar menu start-->
            <ul class="sidebar-menu">
                <li class="active">
                    <a class="" href="index.html">
                        <i class="icon_house_alt"></i>
                        <span>Dashboard</span>
                    </a>
                </li>
            </ul>
            <!-- sidebar menu end-->
        </div>
    </aside>
    <!--sidebar end-->

    <!--main content start-->
    <section id="main-content">
        <section class="wrapper">
            <!--overview start-->
            <div class="row">
                <div class="col-lg-12">
                    <h3 class="page-header"><i class="fa fa-laptop"></i> Dashboard</h3>
                    <ol class="breadcrumb">
                        <li><i class="fa fa-home"></i><a href="index.html">Home</a></li>
                        <li><i class="fa fa-laptop"></i>Dashboard</li>
                        <li><i class="fa fa-folder-open"></i><%= req.query.repo %></li>
                    </ol>
                </div>
            </div>
            <div class="row">
                <form class="navbar-form">
                    <input id="searchPull" onkeyup="pullFilter()" class="form-control" placeholder="Search a pull request" type="text"><br>
                </form>
            </div>
            </br>

            <div class="row">
                <div class="col-lg-12">
                    <section class="panel">


                        <table id="table" cellspacing="0" class="display table table-striped table-advance table-hover ">
                            <thead>
                            <tr>
                                <th><i class="icon_book"></i> Pull Request (open)</th>
                                <th><i class="icon_user"></i> User</th>
                                <th><i class="icon_book"></i> Total pull requests(closed or merged)</th>
                                <th><i class="icon_book"></i> Last update</th>
                                <th><i class="icon_book"></i>Score<span class="required">*</span></th>
                            </tr>
                            </thead>
                            <tbody id="listPull">
                             <% res.forEach(function(ser) { %>
                             <tr>
                                <td><a href="<%= ser.html_url %>"><%= ser.title %></a></td>
                                <td><a href="<%= ser.user.html_url %>"><%= ser.user.login.name %> - (<%= ser.user.login.ratio %>%)</a></td>
                                <td><%= ser.user.login.total %></td>
                                <td><%= ser.updated_at %></td>
                               <% if(ser.user.login.score> 70) { %><td class="alert alert-success"><%= ser.user.login.score %></td><% } else if(ser.user.login.score > 50) { %><td class="alert alert-info"><%= ser.user.login.score %></td><% }else if(ser.user.login.score > 20) { %><td class="alert alert-warning"><%= ser.user.login.score %></td><% } else {%><td class="alert alert-danger"><%= ser.user.login.score %></td><% }%>
                             </tr>
                             <% }); %>
                            </tbody>
                        </table>
                    </section>
                </div>
            </div>
             <div class="row">
                            <div class="col-lg-12">
                                <section class="panel">
                                    <p>* Score calculé via une formule comprenant le nombre de PR, la date d'update de la PR, les statistiques d'ajout et de suppression des differents commits et le ratio du collaborateur.</br>
                                    Ce score peut atteindre la note maximal de 100, les PR disposant du meilleur score sont celles susceptibles d'être intégrer plus rapidement.
                                        A titre d'indice voici le ratio des différentes catégories :</br>
                                        -Nombre de pr : 10%</br>
                                        -Date d'update : 20%</br>
                                        -Statistiques d'ajout et de suppression : 30%</br>
                                        -Ratio du collaborateur : 40%</br>
                                    </p>


                                </section>
                            </div>
                </div>




        </section>
    </section>
    <!--main content end-->
</section>

